<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SVBE - Web</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>CadastroSessao</display-name>
    <servlet-name>CadastroSessao</servlet-name>
    <servlet-class>br.com.svbe.web.servlet.CadastroSessao</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CadastroSessao</servlet-name>
    <url-pattern>/cadastrar-sessao</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CadastraSessaoUsuario</display-name>
    <servlet-name>CadastraSessaoUsuario</servlet-name>
    <servlet-class>br.com.svbe.web.servlet.CadastraSessaoUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CadastraSessaoUsuario</servlet-name>
    <url-pattern>/selecionar-politicos</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
  <error-page>
  	<exception-type>java.lang.Exception</exception-type>
    <location>/erro.jsp</location>
  </error-page>
</web-app>